# Redshift Database Configuration
# Required for both DBT and QA Suite
REDSHIFT_HOST=your-redshift-cluster.region.redshift.amazonaws.com
REDSHIFT_PORT=5439
REDSHIFT_DATABASE=your_database_name
REDSHIFT_DB=your_database_name
REDSHIFT_USER=your_username
REDSHIFT_PASSWORD=your_password

# DBT Specific Configuration
SCHEMA_PREFIX=
USER=your_username

# QA Suite Specific Configuration
TARGET_SCHEMA=your_target_schema

# Optional: Comma-separated list of table prefixes to test
# Leave blank to test all tables in the schema
TABLE_PREFIXES=

# Optional: Comma-separated list of table prefixes to exclude from testing
EXCLUDED_TABLE_PREFIXES=
